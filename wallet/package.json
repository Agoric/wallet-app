{
  "name": "@agoric/wallet-ui",
  "version": "0.5.0",
  "description": "Wallet React frontend",
  "author": "Agoric",
  "license": "Apache-2.0",
  "homepage": "/wallet",
  "type": "module",
  "dependencies": {
    "@agoric/rpc": "^0.0.2-dev-2c6fbc5.0",
    "@agoric/casting": "^0.4.3-dev-6bce049.0",
    "@agoric/cosmic-proto": "0.2.2-dev-a5437cf.0",
    "@agoric/ertp": "^0.16.3-dev-6bce049.0",
    "@agoric/internal": "0.3.2",
    "@agoric/nat": "^4.1.0",
    "@agoric/notifier": "^0.6.2",
    "@agoric/smart-wallet": "^0.5.4-dev-6bce049.0",
    "@agoric/ui-components": "^0.3.9-dev-2c6fbc5.0",
    "@agoric/web-components": "^0.6.4-dev-2c6fbc5.0",
    "@cosmjs/crypto": "^0.31.1",
    "@cosmjs/encoding": "^0.31.1",
    "@cosmjs/proto-signing": "^0.31.1",
    "@cosmjs/stargate": "^0.31.1",
    "@cosmjs/tendermint-rpc": "^0.31.1",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@endo/captp": "^3.1.1",
    "@endo/eventual-send": "^0.17.2",
    "@endo/far": "^0.2.18",
    "@endo/init": "^0.5.56",
    "@endo/marshal": "^0.8.5",
    "@mui/icons-material": "^5.1.0",
    "@mui/lab": "^5.0.0-alpha.67",
    "@mui/material": "^5.1.0",
    "@mui/styles": "^5.1.0",
    "clsx": "^1.1.1",
    "cosmjs-types": "^0.5.0",
    "lodash-es": "^4.17.21",
    "protobufjs": "^7.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-router-dom": "^5.3.0",
    "ses": "^0.18.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@agoric/vats": "^0.15.2-dev-6bce049.0",
    "@keplr-wallet/types": "^0.12.22",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "agoric": "^0.21.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-jsx-a11y": "^6.4.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.0",
    "prettier": "3.0.0",
    "process": "^0.11.10",
    "react-app-rewire-multiple-entry": "^2.2.3",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "source-map-explorer": "^2.5.3",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "@agoric/cosmic-proto": "0.2.2-dev-a5437cf.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "yarn build:ses && react-app-rewired start",
    "build": "yarn build:ses && yarn build:react",
    "build:ses": "cp node_modules/ses/dist/lockdown.umd.js public/",
    "build:react": "react-app-rewired build",
    "lint:format": "yarn prettier --check '**/*.{js,jsx,ts,tsx}'",
    "format": "yarn prettier --write '**/*.{js,jsx,ts,tsx}'",
    "lint": "run-s --continue-on-error lint:*",
    "lint:eslint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:types": "yarn tsc",
    "lint-fix": "yarn lint:eslint --fix",
    "postinstall": "patch-package",
    "test": "CI=true react-app-rewired test",
    "test:watch": "react-app-rewired test",
    "test:xs": "exit 0",
    "test:debug": "react-app-rewired --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "arrowParens": "avoid",
    "trailingComma": "all",
    "singleQuote": true
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!@lit-labs)/"
    ]
  },
  "eslintConfig": {
    "ignorePatterns": [
      "**/*.umd.js",
      "**/generated/*"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "jest"
    ],
    "rules": {
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "**/*.test.*",
            "**/setupTests.js"
          ]
        }
      ],
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off",
      "react/display-name": "off",
      "@typescript-eslint/no-floating-promises": "off"
    },
    "env": {
      "browser": true,
      "jest/globals": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      },
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "@agoric",
      "plugin:react/recommended"
    ]
  },
  "eslintIgnore": [
    "dist/",
    "conf/",
    "build/",
    ".cache",
    "node_modules",
    "config-overrides/"
  ],
  "files": [
    "build/"
  ],
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "public/lib",
        "staticOutDir": "lib"
      },
      {
        "staticPath": "public/assets"
      }
    ],
    "excludeGlob": "**/*.js",
    "watcherGlob": "**/*.html"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
